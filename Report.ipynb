    "    </tr>\n",
    "    <tr>\n",
    "        <td style=\"border: 1px solid black; text-align: center\">\n",
    "            a<br>\n",
    "            a - b<br>\n",
    "            a - (b - c)<br>\n",
    "            (a + b) - c<br>\n",
    "            (a - b)<br>\n",
    "            a - (b - (c + d - (e - f)))<br>\n",
